public class ArrayTest {

    public static void main(String[] args) {

      int[] a;
      a = new int[5];
      int[] b = new int[10];

      a[0] = 15;
      a[1] = 32;
      a[2] = 5555;
      a[3] = 315;
      a[4] = 8;

      for(int i=0;i<a.length;i++) {
        System.out.println(a[i]);
      }
      
    }
    
}
