public class ArrayTest2 {

    public static void main(String[] args) {

    int[] a = {10, 55, 41, 2, 75}; 

    for(int i = 0;i<a.length;i++){
        System.out.println(a[i]);
    }

    System.out.println("---");

    for(int i=a.length-1;i>=0;i--){
        System.out.println(a[i]);

    }

  }

}
